{
  "name": "koa2-demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node bin/www",
    "dev": "NODE_ENV=dev ./node_modules/.bin/gulp dev",
    "pre": "NODE_ENV=pre ./node_modules/.bin/gulp pre",
    "prd": "NODE_ENV=production ./node_modules/.bin/gulp prd",
    "hot-start": "./node_modules/.bin/nodemon bin/www",
    "dev-server": "NODE_ENV=dev ./node_modules/.bin/webpack-dev-server --config ./webpack/webpack.dev.js --progress --hot --colors --inline",
    "dev-build": "NODE_ENV=dev ./node_modules/.bin/webpack --config ./webpack/webpack.dev.js --progress --colors",
    "pre-build": "NODE_ENV=pre ./node_modules/.bin/webpack --config ./webpack/webpack.pre.js --progress --colors",
    "prd-build": "NODE_ENV=production ./node_modules/.bin/webpack --config ./webpack/webpack.prd.js --progress --colors",
    "pm2-dev": "pm2 restart ./pm2.config.js --only koa2 --env dev",
    "pm2-prd": "pm2 start bin/www",
    "test": "NODE_ENV=test ./node_modules/mocha/bin/mocha"
  },
  "dependencies": {
    "debug": "^2.6.3",
    "ejs": "^2.5.7",
    "koa": "^2.2.0",
    "koa-bodyparser": "^3.2.0",
    "koa-convert": "^1.2.0",
    "koa-json": "^2.0.2",
    "koa-jsonp": "^2.0.2",
    "koa-logger": "^2.0.1",
    "koa-mysql-session": "^0.0.2",
    "koa-onerror": "^1.2.1",
    "koa-router": "^7.1.1",
    "koa-session-minimal": "^3.0.4",
    "koa-static": "^3.0.0",
    "koa-views": "^5.2.1",
    "log4js": "^2.3.10",
    "mysql": "^2.15.0",
    "prop-types": "^15.6.0",
    "react": "^16.1.0",
    "react-dom": "^16.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "gulp": "^3.9.1",
    "gulp-nodemon": "^2.2.1",
    "gulp-shell": "^0.6.3",
    "gulp-webpack": "^1.5.0",
    "minimist": "^1.2.0",
    "mocha": "^4.0.1",
    "node-sass": "^4.6.1",
    "nodemon": "^1.8.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "supertest": "^3.0.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4",
    "webpack-merge": "^4.1.1",
    "webpack-stream": "^4.0.0"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "docs/*",
      "logs/*",
      "*.md",
      ".gitignore",
      "build/*"
    ],
    "delay": "2500"
  }
}
